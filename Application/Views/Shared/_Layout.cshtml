        $(document).ready(function () {
            $('.tools').hide();
            $('.tools').css({ "display": "none" });

            $(".page-sidebar-menu a").each(function () {
                var curURL = window.location.pathname;
                curURL = curURL.replace(/\/$/, "");
                curURL = decodeURIComponent(curURL);

                var href = $(this).attr('href');
                if (curURL === href) {
                    $(this).closest('li').addClass('open active');
                    $(this).closest('li').parents("li").addClass('open active');
                    $(this).closest('li').parents("ul").css({ "display": "block" });
                }
            });

            $(".page-sidebar-menu a").each(function () {
                var curURL = window.location.pathname;
                var href1 = $(this).attr('href1');
                var href2 = $(this).attr('href2');
                if ((curURL.indexOf(href1) >= 0) || (curURL.indexOf(href2) >= 0)) {
                    $(this).closest('li').addClass('open active');
                    $(this).closest('li').parents("li").find('.arrow').addClass('open active');
                    $(this).closest('li').parents("li").addClass('open active');
                    $(this).closest('li').parents("ul").css({ "display": "block" });
                }
            }); 
        });

